{
  "name": "spacex",
  "version": "0.0.0",
  "engines": {
    "node": "10.22.1",
    "npm": "6.14.6"
  },
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "build": "ng build --prod --output-hashing=none && npm run concat",
